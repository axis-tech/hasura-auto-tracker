{
  "name": "hasura-auto-tracker",
  "version": "2.3.1",
  "description": "Automatically configure Hasura tracking and generate additional tracked SQL views, e.g. views which can expose JSON data as SQL columns",
  "main": "index.js",
  "bin": {
    "hasura-auto-tracker": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axis-tech/hasura-auto-tracker.git"
  },
  "author": "NamSource Pty Ltd - www.namsource.com",
  "license": "GPL-3.0",
  "scripts": {
    "start_cli": "node cli.js --config example/hasura-auto-tracker.json",
    "start": "nodemon --exec babel-node example/run_hat.js",
    "updatenpm": "npm publish --access=public"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "express": "^4.16.4",
    "nodemon": "^1.18.10"
  },
  "bugs": {
    "url": "https://github.com/axis-tech/hasura-auto-tracker/issues"
  },
  "homepage": "https://github.com/axis-tech/hasura-auto-tracker#readme",
  "keywords": [
    "hasura",
    "tracking",
    "graphql"
  ]
}
